<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xmustang.ssm.rest.mapper.CacheMapper">
    <select id="selectByPrimaryKey" resultType="com.xmustang.ssm.rest.model.Cache">
      select id, `name`, `desc` from cache where id = #{key}
    </select>
</mapper>